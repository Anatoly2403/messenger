{
  "env": {
    "es6": true
  },
  "extends": "airbnb",
  "parser": "@typescript-eslint/eslint-plugin",
  "plugins": ["@typescript-eslint"],
  "rules": {
    "no-var": "error",
    "semi": "error",
    "space-in-parens": "error",
    "prefer-const": "error",
    "no-const-assign": "error",
    "max-len": [1, 100],
    "max-params": [2, 3]
  }
}
